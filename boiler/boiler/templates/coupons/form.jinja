{% extends "layout/defaultlayout.jinja" %}




{% block screen_name %}
  <div class="navbar-brand " href=""><img class="header_image_icon" src="/static/img/coupons.png">Apply Coupons</div>
{% endblock screen_name %}


{% block main_content %}
<div class="col-md-8 order-page pricing">
<div>

   <div class="price-tabs-right">
    <a href="#" class="minorder">Minimum order: Rs.100</a>
  </div>



</div>
      <table class="table clickable-table">
              <tbody data-link="row" class="rowlink">
              <tr>
                <td class="pricing_header">

                  <div class="coupon">
                    <h4>Coupon</h4>
                  </div>

                  <div class="percentage_info">
                    <h4>percentage</h4>
                  </div>

                  <div class="max_amount_info">
                    <h4>max amount</h4>
                  </div>

                  <div class= "min_order_info">
                    <h4>min order</h4>
                  </div>
                </td>
              </tr>

              {% for x in result %}

              {% if x.isActive %}
              <tr>

                <td class="pricing_body">
  
                  <div class="items">
                    <img src="">
                    <h4>{{x.title}}</h4>
                  </div>

                  <div class="price_info">
                    {% if x.price.laundry == "NA" %}
                      <h4>-</h4>
                    {% else %}
                      <h4>Rs.{{x.price.laundry}}</h4>
                    {% endif %}
                  </div>

                  <div class="price_info">
                    {% if x.price.dry_cleaning == "NA" %}
                      <h4>-</h4>
                    {% else %}
                      <h4>Rs.{{x.price.dry_cleaning}}</h4>
                    {% endif %}
                  </div>

                  <div class="price_info">
                    {% if x.price.iron == "NA" %}
                      <h4>-</h4>
                    {% else %}
                      <h4>Rs.{{x.price.iron}}</h4>
                    {% endif %}
                  </div>

                </td>

              </tr>
              {% endif %}
              {% endfor %}



          
                   
              </tbody>
            </table>


{% endblock main_content %}